{
  "name": "svgo",
  "version": "4.0.0",
  "description": "SVG files optimizer",
  "license": "MIT",
  "homepage": "https://github.com/penpot/svgo",
  "bugs": {
    "url": "https://github.com/penpot/svgo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/penpot/svgo.git"
  },
  "files": [
    "src",
    "dist",
    "!**/*.test.js"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build:browser": "browserify -s penpotSvgo ./src/svgo.js > dist/svgo.js && terser --compress -- dist/svgo.js > dist/svgo.min.js",
    "build:node": "esbuild --minify --bundle --platform=node --target=es2021 src/svgoCli.js --outfile=dist/svgo-cli.js"
  },
  "dependencies": {
    "@trysound/sax": "0.2.0",
    "css-select": "^5.1.0",
    "css-tree": "^3.0.0",
    "csso": "^5.0.5",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "browserify": "^17.0.1",
    "esbuild": "^0.24.0",
    "terser": "^5.36.0"
  }
}
